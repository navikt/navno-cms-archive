{
  "name": "navno-cms-archive",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:client": "vite build --outDir ./server/dist/client",
    "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
    "build:server": "npm run build --workspace server",
    "analyze-bundle": "ANALYZE=true npm run build:client",
    "start": "npm run start --workspace server",
    "start-local": "cp .env.prod-local .env && npm run start --workspace server",
    "dev": "cp .env.development .env && npm run dev --workspace server"
  },
  "workspaces": [
    "server"
  ],
  "dependencies": {
    "@mui/material": "5.15.6",
    "@mui/system": "5.15.6",
    "@mui/x-tree-view": "6.17.0",
    "@navikt/aksel-icons": "5.17.4",
    "@navikt/ds-css": "5.17.4",
    "@navikt/ds-react": "5.17.4",
    "csp-header": "5.2.1",
    "js-cookie": "3.0.5",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-xml-viewer": "2.0.1",
    "swr": "2.2.4"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.9",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-css-modules": "2.12.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.4",
    "rollup-plugin-visualizer": "5.12.0",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vite": "5.0.12"
  }
}
