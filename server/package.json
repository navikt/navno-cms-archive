{
  "name": "navno-cms-archive-server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --platform=node --packages=external --outfile=dist/server/server.cjs",
    "start": "node ./dist/server/server.cjs",
    "dev": "npm run build && concurrently \"npm run build -- --watch\" \"nodemon -w ./dist/server -w ../.env ./dist/server/server.cjs\""
  },
  "dependencies": {
    "@opensearch-project/opensearch": "2.5.0",
    "compression": "1.7.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "lru-cache": "10.1.0",
    "mime-types": "2.1.35",
    "node-cache": "5.1.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "concurrently": "8.2.2",
    "nodemon": "3.0.3"
  },
  "nodemonConfig": {
    "delay": 500
  }
}
